        <fullName>Days_Outstanding__c</fullName>
        <description>Days Since Invoiced</description>
        <externalId>false</externalId>
        <formula>IF( Balance__c &lt;=0,0, TODAY() - DATE(YEAR(TxnDate__c),MONTH(TxnDate__c),DAY(TxnDate__c)))</formula>
